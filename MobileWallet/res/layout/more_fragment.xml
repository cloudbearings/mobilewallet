<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/more_fragment_bg_color" >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/more_relative_layout_height"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/learnGK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/more_logo_padding_bottom"
            android:paddingLeft="@dimen/more_logo_padding_left"
            android:paddingRight="@dimen/more_logo_padding_right"
            android:src="@drawable/balance_icon" />

        <View
            android:id="@+id/view1"
            android:layout_width="1.0dip"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/learnGK"
            android:background="@color/line_color" />

        <ImageView
            android:id="@+id/invite"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/view1"
            android:paddingBottom="@dimen/more_logo_padding_bottom"
            android:paddingLeft="@dimen/more_logo_padding_left"
            android:paddingRight="@dimen/more_logo_padding_right"
            android:src="@drawable/invite_icon" />

        <View
            android:id="@+id/view2"
            android:layout_width="1.0dip"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/invite"
            android:background="@color/line_color" />

        <ImageView
            android:id="@+id/recharge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/view2"
            android:paddingBottom="@dimen/more_logo_padding_bottom"
            android:paddingLeft="@dimen/more_logo_padding_left"
            android:paddingRight="@dimen/more_logo_padding_right"
            android:src="@drawable/recharge_icon" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@+id/learnGK"
            android:layout_marginBottom="@dimen/more_logo_textview_margin_bottom"
            android:layout_marginRight="@dimen/learnGK_margin_right"
            android:text="@string/learnGK" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView1"
            android:layout_alignBottom="@+id/textView1"
            android:layout_centerHorizontal="true"
            android:text="@string/invite" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView2"
            android:layout_alignBottom="@+id/textView2"
            android:layout_alignRight="@+id/recharge"
            android:layout_marginRight="@dimen/recharge_margin_right"
            android:text="@string/recharge" />

        <View
            android:id="@+id/view11"
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:background="@color/line_color" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/more_relative_layout_height"
        android:layout_below="@+id/relativeLayout1"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/wallet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/more_logo_padding_bottom"
            android:paddingLeft="@dimen/more_logo_padding_left"
            android:paddingRight="@dimen/more_logo_padding_right"
            android:src="@drawable/wallet_icon" />

        <View
            android:id="@+id/view3"
            android:layout_width="1.0dip"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/wallet"
            android:background="@color/line_color" />

        <ImageView
            android:id="@+id/dailyCredits"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/view3"
            android:paddingBottom="@dimen/more_logo_padding_bottom"
            android:paddingLeft="@dimen/more_logo_padding_left"
            android:paddingRight="@dimen/more_logo_padding_right"
            android:src="@drawable/daily_credits_icon" />

        <View
            android:id="@+id/view4"
            android:layout_width="1.0dip"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/dailyCredits"
            android:background="@color/line_color" />

        <ImageView
            android:id="@+id/notification"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/view4"
            android:paddingBottom="@dimen/more_logo_padding_bottom"
            android:paddingLeft="@dimen/more_logo_padding_left"
            android:paddingRight="@dimen/more_logo_padding_right"
            android:src="@drawable/notification_icon" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@+id/wallet"
            android:layout_marginBottom="@dimen/more_logo_textview_margin_bottom"
            android:layout_marginRight="@dimen/more_logo_textview_margin_right"
            android:text="@string/wallet" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView4"
            android:layout_alignBottom="@+id/textView4"
            android:layout_centerHorizontal="true"
            android:text="@string/daily_credits" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView5"
            android:layout_alignBottom="@+id/textView5"
            android:layout_alignRight="@+id/notification"
            android:layout_marginRight="@dimen/notifications_margin_right"
            android:text="@string/notifications" />

        <View
            android:id="@+id/view12"
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:background="@color/line_color" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout3"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/more_relative_layout_height"
        android:layout_below="@+id/relativeLayout2"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/more_logo_padding_bottom"
            android:paddingLeft="@dimen/more_logo_padding_left"
            android:paddingRight="@dimen/more_logo_padding_right"
            android:src="@drawable/profile_icon" />

        <View
            android:id="@+id/view5"
            android:layout_width="1.0dip"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/profile"
            android:background="@color/line_color" />

        <ImageView
            android:id="@+id/share"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/view5"
            android:paddingBottom="@dimen/more_logo_padding_bottom"
            android:paddingLeft="@dimen/more_logo_padding_left"
            android:paddingRight="@dimen/more_logo_padding_right"
            android:src="@drawable/share_icon" />

        <View
            android:id="@+id/view6"
            android:layout_width="1.0dip"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/share"
            android:background="@color/line_color" />

        <ImageView
            android:id="@+id/rate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/view6"
            android:paddingBottom="@dimen/more_logo_padding_bottom"
            android:paddingLeft="@dimen/more_logo_padding_left"
            android:paddingRight="@dimen/more_logo_padding_right"
            android:src="@drawable/ratings_icon" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@+id/profile"
            android:layout_marginBottom="@dimen/more_logo_textview_margin_bottom"
            android:layout_marginRight="@dimen/about_margin_right"
            android:text="@string/profile" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView7"
            android:layout_alignBottom="@+id/textView7"
            android:layout_centerHorizontal="true"
            android:text="@string/share" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView8"
            android:layout_alignBottom="@+id/textView8"
            android:layout_alignRight="@+id/rate"
            android:layout_marginRight="@dimen/rate_margin_right"
            android:text="@string/rate" />

        <View
            android:id="@+id/view13"
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:background="@color/line_color" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout4"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/more_relative_layout_height"
        android:layout_below="@+id/relativeLayout3"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/feedBack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/more_logo_padding_bottom"
            android:paddingLeft="@dimen/more_logo_padding_left"
            android:paddingRight="@dimen/more_logo_padding_right"
            android:src="@drawable/feedback_icon" />

        <View
            android:id="@+id/view7"
            android:layout_width="1.0dip"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/feedBack"
            android:background="@color/line_color" />

        <ImageView
            android:id="@+id/about"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/view7"
            android:paddingBottom="@dimen/more_logo_padding_bottom"
            android:paddingLeft="@dimen/more_logo_padding_left"
            android:paddingRight="@dimen/more_logo_padding_right"
            android:src="@drawable/about_icon" />

        <View
            android:id="@+id/view8"
            android:layout_width="1.0dip"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/about"
            android:background="@color/line_color" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@+id/feedBack"
            android:layout_marginBottom="@dimen/more_logo_textview_margin_bottom"
            android:layout_marginRight="@dimen/feedback_margin_right"
            android:text="@string/feedback" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView10"
            android:layout_alignBottom="@+id/textView10"
            android:layout_centerHorizontal="true"
            android:text="@string/about" />

        <View
            android:id="@+id/view14"
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:background="@color/line_color" />
    </RelativeLayout>

    <View
        android:id="@+id/view9"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/relativeLayout4"
        android:background="@color/more_view_bg_color" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/relativeLayout4"
        android:layout_toLeftOf="@+id/textView12"
        android:src="@drawable/mobile_icon" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeLayout4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="42.0dip"
        android:text="@string/change_mobile_number" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView12"
        android:layout_marginLeft="35.0dip"
        android:layout_toRightOf="@+id/textView12"
        android:src="@drawable/tm_icon_more_nomal" />

</RelativeLayout>